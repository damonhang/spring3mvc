pipeline {
    agent any
    stages {
        stage('test') {
            steps {
                sh 'mvn test'
            }
        }
        stage('analysis') {
            steps {
                sh 'mvn clean verify sonar:sonar'
            }
        }
        stage('build') {
            steps {
                sh 'mvn --version'
            }
        }
        stage('deploy stage'){
            steps {
                            sh 'echo "deploy stage"'
                        }
        }
    }
}
